alias ls='ls --color=auto'
alias u=ultralist
alias mux=tmuxinator
alias del='~/dotfiles/sybox-config/del'
alias up='source ~/.zshrc'
alias zshrc='$EDITOR ~/.zshrc'
alias vimrc='$EDITOR ~/.vimrc'
alias jupyter='docker run --gpus all --rm -e GRANT_SUDO=yes --user=root --name jupyter -v ~/Data/datascience:/home/jovyan/work -p 8888:8888 jupyter/tensorflow-notebook'
